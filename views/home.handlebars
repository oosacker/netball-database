<table class="table">
    <thead> 
        <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">height</th>
            <th scope="col">hometown</th>
            <th scope="col">team</th>
        </tr>
    </thead>
    <tbody>
        {{#each this}}
        <tr>
            <td scope="row">{{id}}</th>
            <td scope="row">{{name}}</th>
            <td scope="row">{{height}}</th>
            <td scope="row">{{hometown}}</th>
            <td scope="row">{{team}}</th>
        </tr>
        {{/each}}
    </tbody>
</table>
        



